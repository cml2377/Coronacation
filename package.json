{
  "name": "coronacation",
  "version": "1.0.0",
  "description": "VIVA CORONACATIONNNNN",
  "main": "server.js",
  "engines": {
    "node": "12.16.x"
  },
  "scripts": {
    "seed": "node seeders/seed.js",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cml2377/Coronacation.git"
  },
  "keywords": [],
  "author": "Kurt LaVacque, Justin Wofford, Crystal Ly, Brianna McCray, Z Naddaf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cml2377/Coronacation/issues"
  },
  "homepage": "https://github.com/cml2377/Coronacation#readme",
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "concurrently": "^5.1.0",
    "express": "^4.17.1",
    "mongojs": "^3.1.0",
    "mongoose": "^5.9.5",
    "nodemon": "^2.0.2"
  },
  "devDependencies": {
    "node-sass": "^4.13.1"
  }
}